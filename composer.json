{
  "name": "bshp/sonis-php_api",
  "description": "Sonis API Framework for PHP",
  "type": "library",
  "keywords": ["sonis", "jenzabar"],
  "homepage": "https://github.com/bshp/sonis-php_api",
  "license": "MIT",
  "authors": [
    {
      "name": "Jason Everling",
      "email": "jason.everling@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-soap": "*",
    "ext-curl": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "php": ">=7.0.0",
    "ext-soap": "*",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "jms/serializer": "1.7.*",
    "phpdocumentor/phpdocumentor" : "2.*",
    "evert/phpdoc-md" : "~0.2.0",
    "phpunit/phpunit": ">=6.0 <8.0",
    "squizlabs/php_codesniffer": "3.4.2",
    "friendsofphp/php-cs-fixer": "*"
  },
  "autoload": {
    "psr-4": {
      "Jenzabar\\Sonis\\Api\\": "sonis/"
    }
  },
  "autoload-dev": {
    "psr-4" : {
      "Jenzabar\\Sonis\\Api\\Tests\\": "sonis/tests/"
    }
  }
}
